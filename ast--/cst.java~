import java.io.*;
import java.util.*;

public class Ast {
	public List<Decl> decls;
	
	public Program() {
		decls = new LinkedList<Decl>();
	}
	
	public AST(List<Decl> decls) {
		this.decls = decls;
	}
	
	public Add(Decl Newone) {
		delcs.addLast(Newone);
	}
	
	public Program(Decl Newone) {
		decls = new LinkedList<Decl>();
		decls.addLast(Newone);
	}
}
